{
  "name": "ar-io-explorer",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "prettier --write . && eslint --fix && stylelint \"src/**/*.css\" --fix",
    "precommit": "lint-staged",
    "commitlint": "commitlint --edit",
    "clean": "./scripts/clean.sh",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "node:start": "./scripts/start-local-node.sh",
    "node:stop": "./scripts/stop-local-node.sh",
    "node:logs": "docker-compose logs -f ar-io-node",
    "node:restart": "docker-compose restart ar-io-node",
    "postinstall": "husky",
    "prepare": "husky",
    "verify-setup": "echo 'Verifying setup...' && pnpm check && echo 'Testing commitlint...' && echo 'feat: test setup' | pnpm exec commitlint && echo 'âœ… Setup verified successfully!'"
  },
  "dependencies": {
    "@ar.io/sdk": "^3.20.0",
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@faker-js/faker": "^9.6.0",
    "@fontsource-variable/sometype-mono": "^5.2.7",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@permaweb/aoconnect": "^0.0.90",
    "@project-kardeshev/ao-wallet-kit": "^1.0.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/react-table": "^8.21.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "apache-arrow": "^21.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql-request": "^7.2.0",
    "idb": "^8.0.3",
    "js-sha3": "^0.9.3",
    "lucide-react": "^0.476.0",
    "plimit-lit": "^3.0.1",
    "react": "^18.3.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.0.2",
    "tailwind-scrollbar": "^4.0.2",
    "tw-animate-css": "^1.3.6",
    "validator": "^13.15.15",
    "xterm": "^5.3.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/validator": "^13.15.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  },
  "pnpm": {
    "overrides": {
      "@permaweb/aoconnect": "0.0.87"
    }
  }
}
